{
    "globals": {},
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": ["@feedzai/eslint-config-feedzai-react"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
        "react"
    ]
}
